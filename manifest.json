{
    "author": "Alan Files",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "commands": {
        "searchInGoogle": {
            "suggested_key": {
                "default": "Alt+Shift+G",
                "mac": "Command+Shift+G"
            },
            "description": "Search the current query in Google"
        }
    },
    "description": "A browser extension that allows you to easily search google for what you just binged.",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 2,
    "name": "Bing to Google Search",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "version": "1.1",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}